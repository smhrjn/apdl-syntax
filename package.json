{
  "name": "ansys-apdl-syntax",
  "displayName": "ansys-apdl-syntax",
  "description": "syntax highlighting for ansys apdl",
  "version": "0.2.0",
  "publisher": "smhrjn",
  "engines": {
    "vscode": "^1.14.0"
  },
  "icon": "images/ansys.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/smhrjn/apdl-syntax"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "apdl",
        "aliases": [
          "ansys",
          "apdl"
        ],
        "extensions": [
          ".in",
          ".out",
          ".inp",
          ".mac",
          ".ans",
          ".apdl.txt",
          "apdl",
          "ansys"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "apdl",
        "scopeName": "source.apdl",
        "path": "./syntaxes/apdl.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "apdl",
        "path": "./snippets/apdl.json"
      }
    ]
  }
}
